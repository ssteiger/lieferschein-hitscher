{
	"name": "lieferschein-hitscher",
	"description": "Lieferschein Hitscher",
	"version": "0.1.0",
	"authors": ["Soeren Steiger <soeren@memberr.de>"],
	"private": true,
	"packageManager": "npm@10.8.2",
	"workspaces": ["apps/*", "packages/*"],
	"repository": {
		"type": "git",
		"url": "https://github.com/ssteiger/lieferschein-hitscher"
	},
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"dev:db": "cd apps/supabase && npx supabase start",
		"dev:my-app": "turbo run dev --filter=@lieferschein-hitscher/my-app",
		"dev:web": "turbo run dev --filter=@lieferschein-hitscher/web",
		"gen:types": "cd apps/supabase && npm run gen:types",
		"env:check": "node scripts.js env:check"
	},
	"devDependencies": {
		"supabase": "^2.20.5",
		"turbo": "^2.4.4"
	}
}
